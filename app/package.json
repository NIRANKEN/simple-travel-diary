    "cypress:open": "CYPRESS_baseUrl=http://localhost:${CLIENT_PORT:-9000} DISPLAY=\"`grep nameserver /etc/resolv.conf | sed 's/nameserver //' | head -n 1`:0\" cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
